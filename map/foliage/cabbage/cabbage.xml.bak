<?xml version="1.0" encoding="utf-8"?>
<foliageType>
    <!-- cabbage

    0 1 2 3

    1 0 0 0 - invisible
    0 1 0 0 - green small
    1 1 0 0 - green small 2
    0 0 1 0 - green middle
    1 0 1 0 - green big
    0 1 1 0 - harvest ready
    1 1 1 0 - dead
    0 0 0 1 - harvested (extructed)
    1 0 0 1 - cut haulm (not extructed)

    -->
    <foliageLayer cellSize="8" objectMask="16711935" decalLayer="0" densityMapChannelOffset="0" numDensityMapChannels="4" numBlocksPerUnit="1.33" shapeSource="cabbage.i3d">
         <foliageStateDefaults distanceMapLayer="1" width="0.8" height="0.5" widthVariance="0.05" heightVariance="0.05" horizontalPositionVariance="0.15" />
        <!-- default LOD distances etc -->
        <foliageLodDefaults lod="0" viewDistance="18" blendOutDistance="2" atlasSize="1" atlasOffset="1 0" texCoords="0 0 1 1" />
        <foliageLodDefaults lod="1" viewDistance="80" blendOutDistance="2" atlasSize="1" atlasOffset="1 0" texCoords="0 0 1 1" />
        <!-- 1 0 0 0 - invisible -->
        <foliageState name="invisible" />
        <!-- 0 1 0 0 - green small -->
        <foliageState name="green small" distanceMap="distance/foliage_cabbage_distance2_diffuse.png" >
            <foliageShape>
                <foliageLod blockShape="0>0|0" />
                <foliageLod blockShape="0>1|0" />
            </foliageShape>
        </foliageState>
        <!-- 1 1 0 0 - green small 2 -->
        <foliageState name="green small 2" distanceMap="distance/foliage_cabbage_distance2_diffuse.png" >
            <foliageShape>
                <foliageLod blockShape="0>0|0" />
                <foliageLod blockShape="0>1|0" />
            </foliageShape>
        </foliageState>
        <!-- 0 0 1 0 - green middle -->
        <foliageState name="green middle" distanceMap="distance/foliage_cabbage_distance3_diffuse.png" >
            <foliageShape>
                <foliageLod blockShape="0>0|1" />
                <foliageLod blockShape="0>1|1" />
            </foliageShape>
        </foliageState>
        <!-- 1 0 1 0 - green big -->
        <foliageState name="green big" distanceMap="distance/foliage_cabbage_distance4_diffuse.png" >
            <foliageShape>
                <foliageLod blockShape="0>0|2" />
                <foliageLod blockShape="0>1|2" />
            </foliageShape>
        </foliageState>
        <!-- 0 1 1 0 - harvest ready -->
        <foliageState name="harvest ready" distanceMap="distance/foliage_cabbage_distance5_diffuse.png" >
            <foliageShape>
                <foliageLod blockShape="0>0|3" />
                <foliageLod blockShape="0>1|3" />
            </foliageShape>
        </foliageState>
        <!-- 1 1 1 0 - dead -->
        <foliageState name="dead" distanceMap="distance/foliage_cabbage_distance6_diffuse.png" >
            <foliageShape>
                <foliageLod blockShape="0>0|4" />
                <foliageLod blockShape="0>1|4" />
            </foliageShape>
        </foliageState>
        <!-- 0 0 0 1 - harvested -->
    <!-- cabbage haulm -->
    <foliageLayer name="haulm" cellSize="8" objectMask="16711935" decalLayer="0" densityMapChannelOffset="4" numDensityMapChannels="1" numBlocksPerUnit="1.4" shapeSource="cabbageHaulm.i3d">
        <foliageStateDefaults width="1.0" height="1.0" widthVariance="0" heightVariance="0" horizontalPositionVariance="0.1"/>
        <foliageLodDefaults lod="0" viewDistance="80" />
        <foliageState distanceMapLayer="1">
            <foliageShape>
                <foliageLod blockShape="0" atlasSize="1" atlasOffset="1 0" texCoords="0 0 1 1" />
            </foliageShape>
